{
    "name": "flux/fibervent-2",
    "description": "A Symfony 4.4 LTS webapp project to manage Fibervent windmill auditting process",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "a2lix/translation-form-bundle": "^2.0",
        "beberlei/doctrineextensions": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "egulias/email-validator": "^2.1",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^3.0",
        "krixer/google-map-form-type-bundle": "^1.2",
        "lexik/maintenance-bundle": "^2.0",
        "liip/imagine-bundle": "^2.1",
        "mewesk/twig-spreadsheet-bundle": "dev-upgrade-spreadsheet#fc1b231",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.1",
        "snc/redis-bundle": "^3.2",
        "sonata-project/admin-bundle": "^3.52",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/user-bundle": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/yaml": "4.4.*",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.10",
        "whiteoctober/tcpdf-bundle": "^1.0"
    },
    "require-dev": {
        "sensiolabs/security-checker": "^5.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/kgilden/twig-spreadsheet-bundle.git"
        }
    ]
}
