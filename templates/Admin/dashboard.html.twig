{% extends "@SonataAdmin/Core/dashboard.html.twig" %}

{# override css includes #}
{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('calendar') }}
{% endblock %}

{# override javascript includes #}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {callback: 'fos.Router.setData'}) }}"></script>
    {% if app.user.role(constant('App\\Enum\\UserRolesEnum::ROLE_OPERATOR')) or app.user.role(constant('App\\Enum\\UserRolesEnum::ROLE_TECHNICIAN')) %}
        {{ encore_entry_script_tags('calendar') }}
    {% endif %}
{% endblock %}
